          "_customHTML": "<section class=\"pricing4\" group=\"Pricing\" mbr-class=\"{'mbr-parallax-background':bg.parallax}\" plugins=\"mbr-tabs\">\n\n    <mbr-parameters>\n        <header>Size</header>\n        <input type=\"checkbox\" name=\"fullWidth\" title=\"Full Width\">\n        <input type=\"range\" inline title=\"Top\" name=\"paddingTop\" min=\"0\" max=\"10\" step=\"1\" value=\"5\">\n        <input type=\"range\" inline title=\"Bottom\" name=\"paddingBottom\" min=\"0\" max=\"10\" step=\"1\" value=\"5\">\n        <header>Show/Hide</header>\n        <input type=\"checkbox\" title=\"Price\" name=\"showPrice\" checked>\n        <header>Background</header>\n        <fieldset type=\"background\" name=\"bg\">\n            <input type=\"image\" title=\"Image\" value=\"@ADDONS_PATH@/floram4/components/_images/background1.jpg\">\n            <input type=\"color\" title=\"Color\" value=\"#ffffff\" selected>\n        </fieldset>\n        <input type=\"checkbox\" title=\"Overlay\" name=\"overlay\" condition=\"bg.type !== 'color'\" checked>\n        <input type=\"color\" title=\"Overlay Color\" name=\"overlayColor\" value=\"#ffffff\" condition=\"overlay&&bg.type !== 'color'\">\n        <input type=\"range\" inline title=\"Opacity\" name=\"overlayOpacity\" min=\"0\" max=\"1\" step=\"0.1\" value=\"0.7\" condition=\"overlay&&bg.type !== 'color'\">\n    </mbr-parameters>\n\n    <div class=\"mbr-overlay\" mbr-if=\"overlay&&bg.type!== 'color'\" mbr-style=\"{'opacity': overlayOpacity, 'background-color': overlayColor}\">\n    </div>\n    <div mbr-class=\"{'container': !fullWidth, 'container-fluid': fullWidth}\">\n        <div class=\"row justify-content-center\">\n            <div class=\"col-12 col-sm-6 col-lg-4 item features-image\">\n                <div class=\"item-wrapper\">\n                    <div class=\"item-img\">\n                        <img src=\"@PROJECT_PATH@/assets/images/25-removebg-preview-500x500.png\" alt=\"Mobirise\">\n                    </div>\n                    <div class=\"center\" mbr-if=\"showPrice\">\n                        <div class=\"card align-center\">\n                            <p class=\"mbr-text desc1 bord mbr-fonts-style\" mbr-theme-style=\"display-5\" data-app-selector=\".desc1, .iconfont-wrapper\">\n                                PACK DE 25<br><br></p>\n                            <p class=\"mbr-text desc2 bord mbr-fonts-style\" mbr-theme-style=\"display-7\" data-app-selector=\".desc2\">15 EUROS<br>COMMANDEZ</p>\n                        </div>\n                    </div>\n                </div>\n            </div>\n            <div class=\"col-12 col-sm-6 col-lg-4 item features-image\">\n                <div class=\"item-wrapper\">\n                    <div class=\"item-img\">\n                        <img src=\"@PROJECT_PATH@/assets/images/50-removebg-preview-500x500.png\" alt=\"Mobirise\">\n                    </div>\n                    <div class=\"center\" mbr-if=\"showPrice\">\n                        <div class=\"card align-center\">\n                            <p class=\"mbr-text desc1 bord mbr-fonts-style\" mbr-theme-style=\"display-5\" data-app-selector=\".desc1, .iconfont-wrapper\">PACK DE 50 + 50 gratuits</p>\n                            <p class=\"mbr-text desc2 bord mbr-fonts-style\" mbr-theme-style=\"display-7\" data-app-selector=\".desc2\">45 EUROS<br>COMMANDEZ</p>\n                        </div>\n                    </div>\n                </div>\n            </div>\n            <div class=\"col-12 col-sm-6 col-lg-4 item features-image\">\n                <div class=\"item-wrapper\">\n                    <div class=\"item-img\">\n                        <img src=\"@PROJECT_PATH@/assets/images/250-removebg-preview-500x500.png\" alt=\"Mobirise\">\n                    </div>\n                    <div class=\"center\" mbr-if=\"showPrice\">\n                        <div class=\"card align-center\">\n                            <p class=\"mbr-text desc1 bord mbr-fonts-style\" mbr-